<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE validators PUBLIC
"-//OpenSymphony Group//XWork Validator 1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>

	<field name="name">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UN NOMBRE"/>
		</field-validator>
	</field>
	
	<field name="empresa">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UNA EMPRESA"/>
		</field-validator>
	</field>
	
	<field name="email">
		<field-validator type="email">
			<message key="*FORMATO DE EMAIL INCORRECTO"/>
		</field-validator>
	</field>
	
	<field name="telefono">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UN TELEFONO"/>
		</field-validator>
	</field>
	
	<field name="poblacion">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UNA UBICACION "/>
		</field-validator>
	</field>
	
	<field name="pais">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UN PAIS"/>
		</field-validator>
	</field>
	
	<field name="mensaje">
		<field-validator type="requiredstring">
			<message key="*DEBE ESCRIBIR UN MENSAJE"/>
		</field-validator>
	</field>
	
</validators>